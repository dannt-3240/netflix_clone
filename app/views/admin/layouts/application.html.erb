<!DOCTYPE html>
<html>

<head>
  <title>SB Admin 2 - Tables</title>

  <!-- Custom fonts for this template -->
  <link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">

  <!-- Custom styles for this template -->
  <%= stylesheet_link_tag 'admin/sb-admin-2.min' %>
  <%= stylesheet_link_tag 'admin/fontawesome-free/css/all.min.css' %>
  <%= yield(:head) %>

</head>

<body id="page-top">
  <div id="wrapper">
    <!-- Sidebar -->
    <%= render "admin/layouts/left" %>
    <!-- End of Sidebar -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <%= render "admin/layouts/header" %>
        <!-- Begin Page Content -->
        <%= yield %>
        <!-- End of Main Content -->
      </div>
      <!-- Footer -->
        <%= render "admin/layouts/footer" %>

      <!-- End of Footer -->
    </div>
  </div>

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
      aria-hidden="true">
      <div class="modal-dialog" role="document">
          <div class="modal-content">
              <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                  <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">Ã—</span>
                  </button>
              </div>
              <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
              <div class="modal-footer">
                  <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                  <a class="btn btn-primary" href="login.html">Logout</a>
              </div>
          </div>
      </div>
  </div>
  
  <!-- Bootstrap core JavaScript-->
  <script src="/assets/admin/jquery.min.js"></script>
  <script src="/assets/admin/bootstrap.bundle.min.js"></script>


  <!-- Core plugin JavaScript-->
  <script src="/assets/admin/jquery.easing.min.js"></script>


  <!-- Custom scripts for all pages-->
  <script src="/assets/admin/sb-admin-2.min.js"></script>


  <!-- Page level plugins -->
  <script src="/assets/admin/Chart.min.js"></script>


  <!-- Page level custom scripts -->

  <script src="/assets/admin/chart-area-demo.js"></script>
  <script src="/assets/admin/chart-pie-demo.js"></script>

</body>

</html>
